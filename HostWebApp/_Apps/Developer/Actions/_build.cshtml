@using ResponseExtensions = MvcLib.Common.Mvc.ResponseExtensions
@{

    //var assName = string.Format("{0}_Project", DateTime.Now.ToString("yy-MM-dd_hh-mm-ss"));

    var result = EntropiaCompiler.CreateAndSaveAssemblyFromDbFiles(Constants.ASSEMBLY_NAME);
    if (string.IsNullOrEmpty(result))
    {
        ResponseExtensions.WriteAjax(Response, new { success = true, msg = "Assembly compilado com sucesso: " });
    }
    else
    {
        ResponseExtensions.WriteAjax(Response, new { success = false, msg = result });
    }
}